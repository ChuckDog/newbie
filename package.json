{
  "name": "nestjs",
  "version": "0.0.1",
  "description": "",
  "author": "worldzhy",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "gts lint",
    "clean": "gts clean",
    "fix": "gts fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "compile": "tsc",
    "prepare": "npx prisma generate",
    "pretest": "npm run compile",
    "posttest": "npm run lint"
  },
  "prisma": {
    "seed": "ts-node ./prisma/seed.ts"
  },
  "dependencies": {
    "@aws-sdk/client-cloudformation": "^3.112.0",
    "@aws-sdk/client-pinpoint": "^3.76.0",
    "@aws-sdk/client-s3": "^3.76.0",
    "@aws-sdk/client-sns": "^3.76.0",
    "@aws-sdk/client-sqs": "^3.76.0",
    "@aws-sdk/credential-providers": "^3.112.0",
    "@casl/ability": "^6.3.0",
    "@casl/prisma": "^1.3.0",
    "@elastic/elasticsearch": "^7.13.0",
    "@fastify/static": "^6.5.0",
    "@nestjs/common": "^9.1.4",
    "@nestjs/core": "^9.1.4",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.1.4",
    "@nestjs/platform-fastify": "^9.1.4",
    "@nestjs/swagger": "^6.1.2",
    "@prisma/client": "^4.5.0",
    "@pulumi/aws": "5.16.0",
    "@pulumi/pulumi": "3.40.0",
    "axios": "^0.27.2",
    "bcryptjs": "^2.4.3",
    "express": "^4.17.3",
    "lodash": "^4.17.21",
    "passport": "^0.5.2",
    "passport-custom": "^1.1.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "swagger-ui-express": "^4.3.0",
    "typeorm": "^0.3.5",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.0",
    "@commitlint/config-conventional": "^17.0.0",
    "@nestjs/cli": "^9.1.4",
    "@nestjs/schematics": "^9.0.3",
    "@nestjs/testing": "^9.1.4",
    "@types/express": "^4.17.13",
    "@types/jest": "27.4.1",
    "@types/lodash": "^4.14.184",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.0.0",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@types/supertest": "^2.0.11",
    "@types/validator": "^13.7.2",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "gts": "^3.1.0",
    "husky": "^8.0.1",
    "jest": "^27.2.5",
    "prettier": "^2.3.2",
    "prisma": "^4.5.0",
    "source-map-support": "^0.5.20",
    "supertest": "^6.2.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
