{
  "name": "nestjs",
  "version": "0.0.1",
  "description": "",
  "author": "worldzhy",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "gts lint",
    "clean": "gts clean",
    "fix": "gts fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "compile": "tsc",
    "prepare": "npx husky install && npx prisma generate",
    "pretest": "npm run compile",
    "posttest": "npm run lint"
  },
  "prisma": {
    "seed": "ts-node -r tsconfig-paths/register ./prisma/seed.ts"
  },
  "dependencies": {
    "@aws-sdk/client-cloudformation": "^3.316.0",
    "@aws-sdk/client-pinpoint": "^3.316.0",
    "@aws-sdk/client-s3": "^3.317.0",
    "@aws-sdk/client-sqs": "^3.316.0",
    "@aws-sdk/credential-providers": "^3.316.0",
    "@casl/prisma": "^1.4.1",
    "@elastic/elasticsearch": "^7.13.0",
    "@fastify/static": "^6.10.1",
    "@googleapis/forms": "^2.0.5",
    "@nestjs/axios": "^3.0.0",
    "@nestjs/common": "^9.4.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^9.4.0",
    "@nestjs/jwt": "^10.0.3",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.4.0",
    "@nestjs/platform-fastify": "^9.4.0",
    "@nestjs/schedule": "^3.0.1",
    "@nestjs/swagger": "^6.3.0",
    "@nestjs/throttler": "^4.2.1",
    "@prisma/client": "^5.1.0",
    "@pulumi/aws": "5.37.0",
    "@pulumi/pulumi": "3.64.0",
    "axios": "^1.5.1",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cron": "^2.4.0",
    "cron-parser": "^4.8.1",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "multer": "^1.4.5-lts.1",
    "passport": "^0.6.0",
    "passport-custom": "^1.1.1",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pdf-lib": "^1.17.1",
    "rate-limiter-flexible": "^2.4.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.0",
    "rxjs": "^7.8.0",
    "snowflake-sdk": "^1.9.0",
    "stripe": "^12.5.0",
    "swagger-ui-express": "^4.6.2",
    "typeorm": "^0.3.15",
    "validator": "^13.9.0",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.19.3/xlsx-0.19.3.tgz"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.1",
    "@commitlint/config-conventional": "^17.6.1",
    "@nestjs/cli": "^9.4.1",
    "@nestjs/schematics": "^9.1.0",
    "@nestjs/testing": "^9.4.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cron": "^2.0.1",
    "@types/express": "^4.17.17",
    "@types/jest": "29.5.1",
    "@types/lodash": "^4.14.194",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.16.0",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-local": "^1.0.35",
    "@types/snowflake-sdk": "^1.6.14",
    "@types/supertest": "^2.0.12",
    "@types/validator": "^13.7.15",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "gts": "^5.0.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "prettier": "^2.8.7",
    "prisma": "^5.1.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
